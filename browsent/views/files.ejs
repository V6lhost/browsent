<%- include("partials/head", {
  title: "Files that shared with you"
}) %> 
<body>
  <%- include("partials/navbar.ejs", {
    is_active_home: "",
    is_active_files: "active"
  }) %> 
  <% files.forEach(file => { %>
    <div class="card mb-3">
      <div class="row">
        <div class="col-md-3">
          <img src="/images/<%= file.thumbnail %> " alt="" width="100">
        </div>
        <div class="col-md-9">
          <div class="card-body">
            <h5 class="card-title"><%= file.name %></h5>
            <ul>
              <li><a href=/file/<%= file.name %>>File info</a></li>
            </ul>
          </div>
        </div>
      </div>
    </div>
  <% }) %>
</body>
</html>